#:kivy 1.0.9

<TestApp>:
    FloatLayout:
        TextInput:
            id: username
            pos_hint:{"x": 0.1, "top":0.9}
            size_hint:0.8, 0.1
            hint_text: "Account Name: "
        TextInput:
            id: dirname
            pos_hint:{"x": 0.1, "top":0.8}
            size_hint:0.8, 0.1
            hint_text: "Dir Name: "
        Button:
            pos_hint:{"x":0.2, "y": 0.1}
            size_hint:0.6,0.2
            text: "Download"
            on_release: root.do()
